[run]
source = .
omit = 
    */venv/*
    */migrations/*
    */__pycache__/*
    */settings/*
    manage.py
    */test*
    */tests/*
    */admin.py
    */apps.py
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
